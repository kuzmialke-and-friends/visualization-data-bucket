{
  "name": "visualization-api",
  "version": "0.0.0",
  "description": "",
  "main": "app/index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc --project ./tsconfig-build.json",
    "build:watch": "npm run build -- --watch",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watch",
    "start:service": "NODE_PATH=build node build",
    "start": "npm run build && npm run start:service",
    "start:dev": "npm run build && nodemon --watch build --delay 1s --exec 'npm start' & npm run build:watch",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "lint": "eslint --cache --ext .ts --quiet src",
    "prettier:fix": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuzmialke-and-friends/visualization-api.git"
  },
  "author": "",
  "engines": {
    "node": "^v14.16.0",
    "npm": "^6.14.11"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kuzmialke-and-friends/visualization-api/issues"
  },
  "homepage": "https://github.com/kuzmialke-and-friends/visualization-api#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/jest": "^26.0.21",
    "@types/koa": "^2.13.1",
    "@types/koa__cors": "^3.0.2",
    "@types/koa__router": "^8.0.4",
    "@types/node": "^14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "rimraf": "^3.0.2",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "koa": "^2.13.1"
